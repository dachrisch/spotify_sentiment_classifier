{
  "logging": {
    "version": 1,
    "disable_existing_loggers": true,
    "formatters": {
      "single-line": {
        "class": "logging.Formatter",
        "style": "{",
        "datefmt": "%I:%M:%S",
        "format": "[{levelname:s}] {asctime:s} {name:s}::{funcName:s}({lineno:d}): {message:s}"
      }
    },
    "handlers": {
      "debug-console": {
        "level": "DEBUG",
        "class": "logging.StreamHandler",
        "formatter": "single-line",
        "stream": "ext://sys.stdout"
      }
    },
    "loggers": {
      "": {
        "handlers": [
          "debug-console"
        ],
        "level": "DEBUG"
      },
      "spotipy": {
        "handlers": [
          "debug-console"
        ],
        "level": "DEBUG"
      },
      "spotify": {
        "handlers": [
          "debug-console"
        ],
        "level": "INFO"
      },
      "classify": {
        "handlers": [
          "debug-console"
        ],
        "level": "INFO"
      }
    }
  }
}